{
   "useTabs": false,
   "printWidth": 120,
   "tabWidth": 3,
   "singleQuote": false,
   "trailingComma": "es5",
   "jsxBracketSameLine": false,
   "noSemi": false,
   "rcVerbose": true,
   "jsxSingleQuote": false,
   "htmlWhitespaceSensitivity": "css",
   "arrowParens": "always",
   "prettier.cssEnable": ["css", "less", "sass", "scss"],
   "overrides": [
      {
         "files": "*.json",
         "options": { "parser": "json" }
      },
      {
         "files": "*.scss",
         "options": { "parser": "css" }
      },
      {
         "files": ".prettierrc",
         "options": { "parser": "json" }
      }
   ]
}
